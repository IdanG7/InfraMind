# InfraMind Environment Variables

# API Configuration
API_KEY=dev-key-change-in-production
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql://inframind:inframind_dev@localhost:5432/inframind

# Redis
REDIS_URL=redis://localhost:6379/0

# MinIO/S3
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_SECURE=false
MINIO_BUCKET=im-logs

# ML Configuration
MODEL_PATH=./models
MODEL_VERSION=v1
FEATURE_CACHE_TTL=3600
SAFE_MULTIPLIER=1.2
EXPLORATION_RATE=0.15

# Kubernetes (optional)
K8S_NAMESPACE=infra
K8S_IN_CLUSTER=false
